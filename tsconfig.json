{
  "compilerOptions": {
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "incremental": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es2019",
    "lib": [
      "dom",
      "es2019",
      "esnext.asynciterable"
    ],
    "baseUrl": ".",
    "types": ["node"]
  },
  "compileOnSave": false,
  "noResolve": false,
  "noImplicitAny": false
}
